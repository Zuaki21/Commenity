<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/SettingUI.uss?fileID=7433441132597879392&amp;guid=7b48e39b20dc93f4280f898eb14520b7&amp;type=3#SettingUI" />
    <Style src="project://database/Assets/UI/DropdownStyle.uss?fileID=7433441132597879392&amp;guid=043b98f963909834584a108e753a39cd&amp;type=3#DropdownStyle" />
    <ui:VisualElement style="flex-grow: 1; background-color: rgb(233, 233, 233); font-size: 20px;">
        <ui:GroupBox name="GroupBox" style="margin-top: 0; margin-right: 0; margin-bottom: 0; margin-left: 0; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0; flex-direction: row; background-color: rgb(255, 255, 255);">
            <ui:Label text="【設定ウィンドウ】(Escキーで表示/非表示を切り替えられます)" display-tooltip-when-elided="true" name="SettingText" style="font-size: 30px; -unity-font-style: bold; -unity-text-align: middle-left; color: rgb(0, 0, 0); -unity-font-definition: url(&apos;project://database/Assets/Fonts/Text/LINESeedJP_A_TTF_Rg%20SDF.asset?fileID=11400000&amp;guid=f1a3130a04ebb2e46bcdad94c8d0e303&amp;type=2#LINESeedJP_A_TTF_Rg SDF&apos;); flex-grow: 1;" />
            <ui:Button text="X" parse-escape-sequences="true" display-tooltip-when-elided="true" name="CloseButton" style="width: 50px; height: 50px; align-self: center;" />
        </ui:GroupBox>
        <ui:ScrollView name="ScrollView" mouse-wheel-scroll-size="1000" style="-unity-font-definition: url(&apos;project://database/Assets/Fonts/Text/LINESeedJP_A_TTF_Rg%20SDF.asset?fileID=11400000&amp;guid=f1a3130a04ebb2e46bcdad94c8d0e303&amp;type=2#LINESeedJP_A_TTF_Rg SDF&apos;); flex-grow: 1;">
            <ui:GroupBox name="EssentialSettingGroup" style="background-color: rgba(255, 0, 0, 0.22); flex-grow: 1;">
                <ui:Label text="必須項目" display-tooltip-when-elided="true" name="EssentialSettingText" style="-unity-font-style: bold; color: rgb(255, 0, 0); -unity-text-outline-width: 0; -unity-text-outline-color: rgba(0, 0, 0, 0); margin-top: 0; margin-bottom: 0; padding-top: 0; padding-bottom: 0; margin-left: 10px;" />
                <ui:GroupBox name="URLGroup" style="background-color: rgba(255, 0, 10, 0); flex-direction: row; padding-top: 0; padding-bottom: 0; margin-top: 5px; margin-bottom: 5px;">
                    <ui:TextField picking-mode="Ignore" label="Discord Streamkit OverlayのURL" text="URL" name="URLField" placeholder-text="URLを入力してね" style="flex-grow: 0; width: 1145px;" />
                    <ui:Button text="確定" display-tooltip-when-elided="true" name="URLSubmit" />
                </ui:GroupBox>
                <ui:GroupBox style="flex-direction: row;">
                    <ui:Label tabindex="-1" text="URL設定後、「ページ読込中です...」から変わらないなら再起動してみてください→" parse-escape-sequences="true" display-tooltip-when-elided="true" style="align-self: center; color: rgba(27, 27, 27, 0.64);" />
                    <ui:Label tabindex="-1" text="状態：ページ読込中です..." parse-escape-sequences="true" display-tooltip-when-elided="true" name="LoadingChannelText" style="-unity-text-align: upper-right; margin-left: auto; margin-right: 10px;" />
                </ui:GroupBox>
            </ui:GroupBox>
            <ui:Foldout text="詳細設定" value="true" name="DetailSettingFoldout">
                <ui:VisualElement name="DetailSetting" style="flex-grow: 1;">
                    <ui:Toggle label="VOICEVOXでコメントを読み上げる" name="UseVoiceVoxToggle" style="background-color: rgb(150, 154, 255);" />
                    <ui:VisualElement name="ReadGroup" style="flex-grow: 1; background-color: rgb(203, 205, 255);">
                        <ui:RadioButtonGroup label="VOICEVOXの種類&#10;&lt;size=20&gt;(ローカルは反応速度が速いですが、手元PCでのVOICEVOXアプリ起動が必要です)" value="-1" name="VoiceVoxTypeGroup" style="flex-grow: 0; flex-wrap: nowrap;">
                            <ui:RadioButton label="ローカル" name="LocalVoiceVox" style="background-color: rgba(0, 0, 0, 0.12); right: 30px; padding-right: 10px; padding-left: 10px;" />
                            <ui:RadioButton label="Web版" name="WebVoiceVox" style="background-color: rgba(0, 0, 0, 0.12); padding-right: 10px; padding-left: 10px;" />
                        </ui:RadioButtonGroup>
                        <ui:Toggle label="コメントのユーザー名を読み上げる" name="ReadUserName" />
                        <ui:VisualElement name="ReadCharacterGroup" style="flex-grow: 1;">
                            <ui:Label tabindex="-1" text="ロールごとのボイス設定" parse-escape-sequences="true" display-tooltip-when-elided="true" style="-unity-text-align: upper-center; margin-bottom: 0; padding-bottom: 0;" />
                            <ui:GroupBox style="flex-direction: row; margin-bottom: 0; padding-bottom: 0; margin-top: 0; padding-top: 0;">
                                <ui:Button text="Programmer&#10;&lt;size=15&gt;ずんだもん/ノーマル" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ProgrammerVoice" style="flex-grow: 1; text-overflow: ellipsis;" />
                                <ui:Button text="Illustrator" parse-escape-sequences="true" display-tooltip-when-elided="true" name="IllustratorVoice" style="flex-grow: 1; text-overflow: ellipsis;" />
                                <ui:Button text="SoundCreator" parse-escape-sequences="true" display-tooltip-when-elided="true" name="SoundCreatorVoice" style="flex-grow: 1; text-overflow: ellipsis;" />
                            </ui:GroupBox>
                            <ui:GroupBox name="GroupBox" style="flex-direction: row; margin-top: 0;">
                                <ui:Button text="ScenarioWriter" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ScenarioWriterVoice" style="flex-grow: 1; text-overflow: ellipsis;" />
                                <ui:Button text="ChatGPT" parse-escape-sequences="true" display-tooltip-when-elided="true" name="GPTVoice" style="flex-grow: 1; text-overflow: ellipsis;" />
                                <ui:Button text="Other" parse-escape-sequences="true" display-tooltip-when-elided="true" name="OtherVoice" style="flex-grow: 1; text-overflow: ellipsis;" />
                            </ui:GroupBox>
                        </ui:VisualElement>
                        <ui:VisualElement name="ReadSettingGroup" style="flex-grow: 1; margin-right: 10px; margin-left: 10px; margin-bottom: 10px; background-color: rgb(230, 231, 255); padding-right: 30px; padding-left: 30px;">
                            <ui:Label tabindex="-1" text="読み上げ全体の設定" parse-escape-sequences="true" display-tooltip-when-elided="true" style="-unity-text-align: upper-center; margin-bottom: 0; padding-bottom: 0;" />
                            <ui:Slider picking-mode="Ignore" label="読み上げ速度" value="1.25" high-value="2" low-value="0.5" show-input-field="true" name="ReadSpeedSlider" />
                            <ui:Slider picking-mode="Ignore" label="ピッチ" value="0" high-value="0.1" low-value="-0.1" show-input-field="true" name="ReadPitchSlider" />
                            <ui:Slider picking-mode="Ignore" label="イントネーション幅" value="1" high-value="2" low-value="0" show-input-field="true" name="ReadIntonationScaleSlider" />
                            <ui:SliderInt picking-mode="Ignore" label="1コメントの最大文字量" value="50" high-value="100" low-value="10" show-input-field="true" />
                            <ui:SliderInt picking-mode="Ignore" label="読み上げ待ちの最大文字量" value="100" high-value="200" low-value="20" show-input-field="true" />
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:Toggle label="AI(ChatGPT)でコメントを増やす" name="UseAIToggle" style="align-items: stretch; justify-content: flex-start; -unity-text-align: upper-left; flex-direction: row; background-color: rgb(144, 231, 112); margin-top: 20px;" />
                    <ui:VisualElement name="AIGroup" style="flex-grow: 1; background-color: rgb(190, 233, 195);">
                        <ui:GroupBox name="GPTAPIGroup" style="flex-direction: row;">
                            <ui:TextField picking-mode="Ignore" text="APIキー" name="ChatGPTAPIField" placeholder-text="ChatGPTのAPIキー" label="ChatGPTのAPIキー&#10;&lt;size=20&gt;(設定後、再起動してください)" style="flex-grow: 0; width: 1145px;" />
                            <ui:Button text="確定" display-tooltip-when-elided="true" name="ChatGPTAPISubmit" />
                        </ui:GroupBox>
                        <ui:Toggle label="ChatGPTのコメントを読み上げる" name="ReadAIComments" style="height: 63px;" />
                    </ui:VisualElement>
                </ui:VisualElement>
            </ui:Foldout>
            <ui:Button text="デバッグコンソールを開く" display-tooltip-when-elided="true" name="ConsoleLogButton" style="font-size: 20px; width: 400px; align-self: flex-end; margin-top: 10px;" />
            <ui:Button text="初期設定に戻す(再起動します)" display-tooltip-when-elided="true" name="ResetButton" style="font-size: 20px; width: 400px; align-self: flex-end; margin-top: 10px;" />
        </ui:ScrollView>
    </ui:VisualElement>
</ui:UXML>
